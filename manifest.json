{
	"manifest_version": 3,
	"name": "Start Web Application",
	"short_name": "StartWeb",
	"description": "Minimal Homepage",
	"version": "0.0.1",
	"chrome_url_overrides": {
		"newtab": "./index.html"
	},
	"icons": [
		{
			"src": "/icons/icon.png",
			"sizes": "192x192",
			"type": "image/png"
		},
		{
			"src": "/icons/icon.png",
			"sizes": "512x512",
			"type": "image/png"
		}
	],
	"start_url": "/",
	"display": "standalone",
	"theme_color": "#000000",
	"background_color": "#ffffff"
}
